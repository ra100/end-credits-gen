{
  "name": "@ra100-ecg/svg",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "start": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc -b",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist"
      ],
      "clean": "if-file-deleted"
    },
    "start": {
      "command": "node dist/index.js",
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "command": "jest",
      "files": [
        "src/**/*.ts"
      ]
    }
  },
  "author": "ra100",
  "license": "GNU LGPLv3",
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.238.0",
    "@aws-sdk/client-sqs": "^3.238.0",
    "nanoid": "~3.3.4"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.226.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
