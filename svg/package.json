{
  "name": "@ra100-ecg/svg",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "start": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc -b",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist"
      ],
      "clean": "if-file-deleted"
    },
    "start": {
      "command": "node dist/index.js",
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "command": "jest",
      "files": [
        "src/**/*.ts"
      ]
    }
  },
  "author": "ra100",
  "license": "GNU LGPLv3",
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.359.0",
    "@aws-sdk/client-sqs": "^3.359.0",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.357.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
