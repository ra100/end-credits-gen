{
  "name": "@ra100-ecg/result",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "start": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc -b",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist"
      ],
      "clean": "if-file-deleted",
      "depencencies": [
        "../svg:build"
      ]
    },
    "start": {
      "command": "node dist/index.js",
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "command": "jest",
      "files": [
        "src/**/*.ts"
      ]
    }
  },
  "author": "ra100",
  "license": "GNU LGPLv3",
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.651.1",
    "@aws-sdk/client-s3": "^3.651.1",
    "@aws-sdk/lib-storage": "^3.651.1",
    "@aws-sdk/s3-request-presigner": "^3.651.1",
    "@ra100-ecg/svg": "^1.0.0",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1691.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.649.0",
    "@types/archiver": "^6.0.2",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.13",
    "@types/node": "22.7.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
