{
  "name": "@ra100-ecg/png",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/handler.js",
  "module": "dist/handler.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "start": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc -b",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist"
      ],
      "clean": "if-file-deleted",
      "depencencies": [
        "../svg:build"
      ]
    },
    "start": {
      "command": "node dist/index.js",
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "command": "jest",
      "files": [
        "src/**/*.ts"
      ]
    }
  },
  "author": "ra100",
  "license": "GNU LGPLv3",
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.90.0",
    "@aws-sdk/client-s3": "^3.88.0",
    "@aws-sdk/lib-storage": "^3.88.0",
    "@ra100-ecg/svg": "^1.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.78.0",
    "@types/aws-lambda": "^8.10.97",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
